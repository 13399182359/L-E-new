                <dl class="lineD" id='dl_old_price'>
                    <dt>原价：</dt>
                    <dd>

                        <span style="text-decoration:line-through;">￥${order.bazaarprice}</span>

                    </dd>
                </dl>

                <input name="discount" id="form_discount" type="hidden" value="-" class="s-txt">

                <dl class="lineD" id='dl_discount_type'>
                    <dt>折扣类型：</dt>
                    <dd>

                        <#if order.discounttype=1><span style="color:green">系统赠送</span>
                        <#else ><span style="color:#7f8880">无折扣</span></#if>

                    </dd>
                </dl>
                <dl class="lineD" id='dl_price'>
                    <dt>购买价格：</dt>
                    <dd>

                        <span style="color:red">￥${order.price}</span>

                    </dd>
                </dl>
                <dl class="lineD" id='dl_pay_status'>
                    <dt>支付状态：</dt>
                    <dd>

                        <#if order.status=1><span style="color:green">支付成功</span>
                        <#else ><span style="color:red">未支付</span></#if>

                    </dd>
                </dl>

                <input name="learn_status" id="form_learn_status" type="hidden" value="未开始" class="s-txt">

                <div class="page_btm" >
                    <input type='text' style="display:none">
                    <input type="button" class="btn_b" value="返 回" id='form_submit' onclick = "submitform()"/>

                </div>
                </#list>
            </div>
        </form>

        <!-- END CONFIG_FORM -->
    </div>
    <script>
        function submitform(){
            $('[name="detail_form"]').submit();
        }
    </script>
    <script type="text/javascript">
        document.onkeydown=function (e) {
            e=window.event||e;
            var key = e.keyCode;
            if ((e.ctrlKey && key == 82) || key == 116) {
                parent.MainIframe.location.reload();
                if(document.all) {
                    e.keyCode = 0;
                    e.returnValue = false;
                }else {
                    e.cancelBubble = true;
                    e.preventDefault();
                }
            }
        }
        /**
         * 初始化对象
         */
//表格样式
        $(document).ready(function(){
            admin.bindTrOn();});
    </script>

</body>
</html>