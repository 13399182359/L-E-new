    exportContentList = function () {
        var arr = $("[name='checkbox']:checked");
        if (arr.length > 0) {
            var ids = '';
            for (var i = 0; i <arr.length ; i++) {
                ids += ids == ''?arr[i].value:','+arr[i].value;
            }
            location.href = "/poi/exportContentList?ids="+ids
        }else {
