        ids= ids.substr(1);
        if ( ids!= null && ids!="") {
            $.post("/delBankById",{ids:ids},function (data) {
                alert(data.message)
                location.href="/queryBank";
            })
        }else {
            alert("请选择要删除的数据！！")
        }
